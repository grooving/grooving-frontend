.get(endpoints.artistPersonalInformation)\n                .then(response => {\n                    var personalInformation = response.data.user;\n                    console.log(personalInformation);\n                    console.log(response);\n                    \n                    this.userName=personalInformation['first_name']\n                    this.userSurnames = personalInformation['last_name'];\n                    this.userEmail=personalInformation['email'];\n                    this.userPhoneNumber = response.data.phone;\n\n\n                \n            });\n        \n        }\n    }\n}\n</script>\n\n<style scoped>\n    * {\n        font-family: \"Archivo\"\n    }\n\n    @media (min-width:768px)  {\n      \n        .everything {\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            margin-top: 5%;\n            text-align: center;\n            padding: 15px;\n            margin-left: 10%;\n            margin-right: 10%;\n            margin-top:0%;\n        }\n    }\n\n</style>\n"]}]}